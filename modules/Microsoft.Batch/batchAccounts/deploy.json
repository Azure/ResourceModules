      },
      "allowedValues": [
        "BatchAccountManagedIdentity",
        "StorageKeys"
      ]
    },
    "storageAccessIdentity": {
      "type": "string",
      "defaultValue": "",
      "metadata": {
        "description": "Optional. The resource ID of a user assigned identity assigned to pools which have compute nodes that need access to auto-storage."
